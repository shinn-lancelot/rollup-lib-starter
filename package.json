{
  "name": "rollup-lib-starter",
  "version": "0.0.0",
  "description": "A project template which using Rollup to create a library.一个使用Rollup打包工具来编写js库的项目模板。",
  "main": "dist/rolluplib.cjs.js",
  "module": "dist/rolluplib.esm.js",
  "browser": "dist/rolluplib.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:init": "eslint --init",
    "lint": "eslint --fix --ext .js src",
    "commit": "git-cz",
    "dev": "npm run watch",
    "watch": "rollup -c -w",
    "build": "rollup -c"
  },
  "keywords": [
    "rollup-lib-starter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shinn-lancelot/rollup-lib-starter.git"
  },
  "bugs": {
    "url": "https://github.com/shinn-lancelot/rollup-lib-starter/issues"
  },
  "readme": "README.md",
  "author": "shinn_lancelot",
  "license": "MIT",
  "devDependencies": {
    "commitizen": "^3.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.11.1",
    "rollup": "^0.67.4",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-flow-no-whitespace": "^1.0.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "^3.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
