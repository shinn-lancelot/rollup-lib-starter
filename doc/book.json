{
    "title": "rollup-lib-starter",
    "author": "shinn_lancelot",
    "description": "rollup-lib-starter document",
    "language": "en",
    "gitbook": "^3.2.3",
    "structure": {
        "readme": "readme.md"
    },
    "plugins": [
        "-sharing",
        "search",
        "sharing-plus",
        "search-pro",
        "expandable-chapters-small",
        "anchors",
        "anchor-navigation-ex",
        "github",
        "github-buttons",
        "edit-link",
        "donate",
        "theme-vuejs",
        "scripts",
        "versions-select"
    ],
    "pluginsConfig": {
        "sharing": {
            "all": [
                "facebook",
                "google",
                "qq",
                "twitter",
                "weibo"
            ]
        },
        "search-pro": {
            "cutWordLib": "nodejieba",
            "defineWord" : ["Gitbook Use"]
        },
        "anchor-navigation-ex": {
            "showLevel": false
        },
        "github": {
            "url": "https://github.com/shinn-lancelot/rollup-lib-starter"
        },
        "github-buttons": {
            "buttons": [
                {
                    "user": "shinn-lancelot",
                    "repo": "rollup-lib-starter",
                    "type": "star",
                    "size": "small",
                    "count": true
                }
            ]
        },
        "donate": {
            "wechat": "",
            "alipay": "",
            "title": "",
            "button": "Donate",
            "wechatText": "WeChat Pay",
            "alipayText": "Alipay"
        },
        "edit-link": {
            "base": "https://github.com/shinn-lancelot/rollup-lib-starter/tree/master/doc",
            "label": "Edit This Page"
        },
        "versions": {
            "options": [
                {
                    "value": "https://hillpy.github.io/rollup-lib-starter/",
                    "text": "v0.0.0"
                }
            ]
        }
    }
}